{
    "name": "nwidart/money-formatter-laravel",
    "description": "Laravel Money formatter integration",
    "keywords": [
        "money-formatter-laravel"
    ],
    "homepage": "https://github.com/nwidart/money-formatter-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Widart",
            "email": "n.widart@gmail.com",
            "homepage": "http://www.nicolaswidart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~5.0",
        "nwidart/money-formatter": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Nwidart\\MoneyFormatterLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nwidart\\MoneyFormatterLaravel\\tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
